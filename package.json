{
  "name": "wingmate",
  "displayName": "Wingmate â€” SQL tools",
  "description": "SQL syntax highlighting and syntax error detection within strings in Go code",
  "publisher": "chrismwendt",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrismwendt/wingmate.git"
  },
  "version": "1.0.16",
  "private": true,
  "engines": {
    "vscode": "^1.63.0"
  },
  "main": "./out/main.js",
  "activationEvents": [
    "onLanguage:go"
  ],
  "icon": "images/icon.png",
  "contributes": {
    "commands": [
      {
        "command": "wingmate.refreshSchema",
        "title": "Wingmate: Refresh Schema"
      }
    ],
    "configuration": {
      "title": "Wingmate",
      "properties": {
        "wingmate.sinks": {
          "type": "array",
          "default": [
            "ExecContext:1",
            "PrepareContext:1",
            "QueryContext:1",
            "QueryRowContext:1",
            "Exec:0",
            "Prepare:0",
            "Query:0",
            "QueryRow:0",
            "sqlf.Sprintf:0"
          ],
          "description": "Arguments to functions that will be treated as SQL."
        },
        "wingmate.conn": {
          "type": "string",
          "default": "postgresql://localhost:5432/postgres",
          "description": "Postgres connection string."
        }
      }
    },
    "semanticTokenScopes": [
      {
        "scopes": {
          "sqlkeyword": [
            "keyword.other.DML.sql"
          ],
          "sqlstring": [
            "string.quoted.single.sql"
          ],
          "sqlnumber": [
            "constant.numeric.sql"
          ],
          "sqlasterisk": [
            "keyword.operator.star.sql"
          ],
          "sqlcomment": [
            "comment.line.double-dash.sql"
          ],
          "sqlidentifier": [
            "entity.name.function.sql"
          ],
          "sqloperator": [
            "keyword.operator.comparison.sql"
          ],
          "sqlwhitespace": [
            "punctuation.whitespace.comment.leading.sql"
          ]
        }
      }
    ]
  },
  "dependencies": {
    "immutable": "",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "rxjs": "",
    "typescript": "^4.8.4",
    "web-tree-sitter": "0.20.7"
  },
  "devDependencies": {
    "@tsconfig/node18": "",
    "@types/lodash": "^4.14.188",
    "@types/node": "18.11.9",
    "@types/pg": "^8.6.5",
    "@types/vscode": "1.63.1",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "esbuild": "^0.15.14",
    "eslint": "^8.27.0",
    "tree-sitter-cli": "0.20.7",
    "vsce": ""
  }
}
